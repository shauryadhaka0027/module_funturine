{
  "name": "module_funturine",
  "version": "1.0.0",
  "description": "Moulded Furniture API - A comprehensive furniture management system",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "test": "jest",
    "lint": "eslint **/*.ts --ignore-pattern node_modules --ignore-pattern dist",
    "lint:fix": "eslint **/*.ts --fix --ignore-pattern node_modules --ignore-pattern dist",
    "init-db": "ts-node scripts/init-db.ts",
    "create-admin": "ts-node scripts/create-admin.ts",
    "populate-products": "ts-node scripts/populate-products.ts",
    "test-email": "ts-node scripts/test-email.ts",
    "install-setup": "npm install && npm run build",
    "render-build": "npm install && npm run build",
    "render-start": "node dist/server.js"
  },
  "keywords": [
    "furniture",
    "api",
    "nodejs",
    "typescript",
    "mongodb",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",

    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.7",
    "@types/cors": "^2.8.15",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.7",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}